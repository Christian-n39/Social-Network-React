>\n      {\n        Object.keys(post).length !== 0 ? <SinglePost post={post} createComment={createComment} /> : ''\n      }\n    </>\n  )\n}","/home/christian_n39/Dev/DesarrolloWeb/socialMERN/frontend/src/components/SinglePost/index.jsx",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":8,"column":8,"nodeType":"112","messageId":"113","endLine":8,"endColumn":22},{"ruleId":"114","severity":1,"message":"115","line":25,"column":54,"nodeType":"116","messageId":"117","endLine":25,"endColumn":56},{"ruleId":"118","severity":1,"message":"119","line":31,"column":6,"nodeType":"120","endLine":31,"endColumn":8,"suggestions":"121"},{"ruleId":"114","severity":1,"message":"115","line":34,"column":19,"nodeType":"116","messageId":"117","endLine":34,"endColumn":21},{"ruleId":"114","severity":1,"message":"115","line":27,"column":54,"nodeType":"116","messageId":"117","endLine":27,"endColumn":56},{"ruleId":"118","severity":1,"message":"119","line":34,"column":6,"nodeType":"120","endLine":34,"endColumn":8,"suggestions":"122"},{"ruleId":"123","severity":1,"message":"124","line":38,"column":9,"nodeType":"125","endLine":38,"endColumn":393},{"ruleId":"114","severity":1,"message":"115","line":25,"column":54,"nodeType":"116","messageId":"117","endLine":25,"endColumn":56},{"ruleId":"114","severity":1,"message":"115","line":44,"column":54,"nodeType":"116","messageId":"117","endLine":44,"endColumn":56},{"ruleId":"114","severity":1,"message":"115","line":54,"column":58,"nodeType":"116","messageId":"117","endLine":54,"endColumn":60},{"ruleId":"114","severity":1,"message":"115","line":54,"column":132,"nodeType":"116","messageId":"117","endLine":54,"endColumn":134},{"ruleId":"110","severity":1,"message":"126","line":14,"column":9,"nodeType":"112","messageId":"113","endLine":14,"endColumn":18},{"ruleId":"127","severity":1,"message":"128","line":21,"column":19,"nodeType":"129","messageId":"130","endLine":21,"endColumn":20},{"ru